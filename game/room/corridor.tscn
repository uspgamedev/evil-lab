script/source = "extends Node2D

const MONSTER = preload(\"res://objects/monster/flickering_monster.tscn\")

func spawn():
	var monster = MONSTER.instance()
	monster.set_pos(get_pos())
	monster.character = get_parent().character
	get_parent().add_child(monster)
	queue_free()
"

[sub_resource type="GDScript" id=3]

script/source = "extends Node

const SPEECH = preload(\"res://hud/speech/basement_speech.tscn\")

onready var unlock_event = get_node(\"/root/Main/Events/UnlockBasement\")
onready var door = get_parent().get_node(\"BasementDoor\")

func _ready():
	unlock_event.door = door

func nope():
	if get_child_count() == 0:
		var speech = SPEECH.instance()
		add_child(speech)
		yield(get_tree(), \"fixed_frame\")
		speech.go()
"

[sub_resource type="GDScript" id=4]

script/source = "extends Timer

onready var knock = get_parent().get_node(\"OfficeDoor/SFX\")

func go():
	knock.go()
	set_wait_time(2 + randf()*3)
	start()
"

